[
  { "id": 101, "english": "I'm speechless.", "korean": "말문이 막히네, 할 말을 잃었어." },
  { "id": 102, "english": "I don't know a thing.", "korean": "난 아무것도 몰라." },
  { "id": 103, "english": "Where was I?", "korean": "내가 어디까지 이야기했지?" },
  { "id": 104, "english": "Wanna grab some lunch?", "korean": "점심 먹을래?" },
  { "id": 105, "english": "I had a rough night.", "korean": "밤잠을 설쳤어." },
  { "id": 106, "english": "This can't be happening.", "korean": "이건 있을 수 없는 일이야." },
  { "id": 107, "english": "Now you're talking.", "korean": "이제야 말이 통하네!" },
  { "id": 108, "english": "You got that right.", "korean": "네 말이 맞아." },
  { "id": 109, "english": "I don't see that happening.", "korean": "그렇게는 안될 걸." },
  { "id": 110, "english": "Not that I know of.", "korean": "내가 알기로는 그렇지 않아." },
  { "id": 111, "english": "Over my dead body.", "korean": "절.대.안.돼." },
  { "id": 112, "english": "I'm hoping for the best.", "korean": "잘 되길 빌고 있어." },
  { "id": 113, "english": "I'm down.", "korean": "난 찬성이야." },
  { "id": 114, "english": "No way.", "korean": "말도 안돼. / 헐 대박." },
  { "id": 115, "english": "Mind if I join you?", "korean": "내가 껴도 돼?" },
  { "id": 116, "english": "Who cares?", "korean": "알게 뭐람? 무슨 상관이야?" },
  { "id": 117, "english": "It's not my place.", "korean": "내가 나설 자리가 아냐." },
  { "id": 118, "english": "Follow your heart.", "korean": "마음 가는대로 해." },
  { "id": 119, "english": "I wouldn't if I were you.", "korean": "내가 너라면 그렇게 하지 않겠어." },
  { "id": 120, "english": "Do your job.", "korean": "네 일이나 잘해." },
  { "id": 121, "english": "It's all or nothing.", "korean": "모 아니면 도야." },
  { "id": 122, "english": "Behave yourself.", "korean": "버릇없이 굴면 안돼. / 점잖게 행동해." },
  { "id": 123, "english": "What's in it for me?", "korean": "내가 얻는 게 뭔데?" },
  { "id": 124, "english": "Shame on you!", "korean": "부끄러운 줄 알아야지! 창피한 일이야!" },
  { "id": 125, "english": "You're one to talk.", "korean": "사돈 남 말 하네." },
  { "id": 126, "english": "You asked for it.", "korean": "자업자득이지." },
  { "id": 127, "english": "It serves you right!", "korean": "꼴 좋다! 넌 그런 일 당해도 싸!" },
  { "id": 128, "english": "You catch on quick.", "korean": "눈치가 빠르네. / 이해가 빠르네." },
  { "id": 129, "english": "Don't blow it.", "korean": "기회를 날리지 마. / 망치지 마." },
  { "id": 130, "english": "Then so be it.", "korean": "그럼 그렇게 해. / 어쩔 수 없지." },
  { "id": 131, "english": "Watch your tongue!", "korean": "말 조심해!" },
  { "id": 132, "english": "I want no part of it.", "korean": "난 그 일에 관여하고 싶지 않아." },
  { "id": 133, "english": "Please don't butt in.", "korean": "끼어들지 마." },
  { "id": 134, "english": "Brace yourself.", "korean": "마음 단단히 먹고 들어." },
  { "id": 135, "english": "Let's just call it even.", "korean": "비긴 셈치자." },
  { "id": 136, "english": "It's a guy thing.", "korean": "남자들 이야기야." },
  { "id": 137, "english": "She's gonna mooch off us.", "korean": "걔는 우리에게 빌붙어살거야." },
  { "id": 138, "english": "The same goes for you.", "korean": "너도 마찬가지야." },
  { "id": 139, "english": "Pull over right here.", "korean": "바로 여기에 차를 세워요." },
  { "id": 140, "english": "Hop in.", "korean": "어서 타." },
  { "id": 141, "english": "Going my way?", "korean": "혹시 나랑 같은 방향으로 가니?" },
  { "id": 142, "english": "You can't miss it.", "korean": "쉽게 찾을 수 있을 거야." },
  { "id": 143, "english": "I'm stuck in traffic.", "korean": "차가 막혀." },
  { "id": 144, "english": "I can't afford it.", "korean": "그럴 형편이 안돼." },
  { "id": 145, "english": "Take it up with your boss.", "korean": "사장하고 이야기해봐." },
  { "id": 146, "english": "Would you scoot a little?", "korean": "조금만 옆으로 가줄래요?" },
  { "id": 147, "english": "I can hold my head high.", "korean": "난 떳떳할 수 있어." },
  { "id": 148, "english": "That's all I need.", "korean": "내가 필요한 건 그게 다야." },
  { "id": 149, "english": "I'm doing my hair.", "korean": "머리 손질 중이야." },
  { "id": 150, "english": "Can you break a hundred?", "korean": "100불을 잔돈으로 바꿔주실래요?" },
  { "id": 151, "english": "What an imagination you have.", "korean": "상상력도 풍부하군!" },
  { "id": 152, "english": "We've grown apart.", "korean": "성장하면서 자연스레 멀어졌어." },
  { "id": 153, "english": "I just feel drained.", "korean": "기운이 다 빠진 것 같아요." },
  { "id": 154, "english": "Get a grip!", "korean": "정신 차려!" },
  { "id": 155, "english": "Can I tag along?", "korean": "나도 따라가도 돼?" },
  { "id": 156, "english": "It's a hassle.", "korean": "귀찮아. / 번거로워." },
  { "id": 157, "english": "So far, so good.", "korean": "지금까지는 괜찮아." },
  { "id": 158, "english": "You haven't changed a bit.", "korean": "너 정말 하나도 안 변했구나." },
  { "id": 159, "english": "I'm laying low.", "korean": "혼자 조용히 지내고 있어." },
  { "id": 160, "english": "Rock on!", "korean": "멋지군! 좋아!" },
  { "id": 161, "english": "It's hard to keep up.", "korean": "따라잡기 어려워." },
  { "id": 162, "english": "That makes two of us.", "korean": "나도 마찬가지야." },
  { "id": 163, "english": "There, there.", "korean": "(위로할 때) 토닥토닥/괜찮아" },
  { "id": 164, "english": "It was all for nothing.", "korean": "괜히 했어." },
  { "id": 165, "english": "There wasn't time.", "korean": "시간 없었어." },
  { "id": 166, "english": "I'm in a bind.", "korean": "난처한 상황에 처해있어." },
  { "id": 167, "english": "He's always on the go.", "korean": "그는 늘 바쁘게 돌아다녀." },
  { "id": 168, "english": "I smell a rat.", "korean": "수상해. 뭔가 냄새가 나." },
  { "id": 169, "english": "I could eat a horse.", "korean": "(배가 고파서) 뭐든지 먹을 수 있어." },
  { "id": 170, "english": "You're all missing my point.", "korean": "내 요점을 모르네." },
  { "id": 171, "english": "Pardon my language.", "korean": "거친 표현 미안해." },
  { "id": 172, "english": "Let's just talk turkey.", "korean": "본론으로 들어가죠." },
  { "id": 173, "english": "I'll come to the point.", "korean": "단도직입적으로 말할 게." },
  { "id": 174, "english": "It carries no weight.", "korean": "그건 전혀 중요하지 않아." },
  { "id": 175, "english": "Put your feet up.", "korean": "푹 쉬어." },
  { "id": 176, "english": "Drop the subject.", "korean": "그 얘긴 그만해." },
  { "id": 177, "english": "Everything's up in the air.", "korean": "아직 모든 것이 미정이야." },
  { "id": 178, "english": "Thanks for walking me home.", "korean": "집까지 같이 걸어와줘서 고마워." },
  { "id": 179, "english": "Speak of the devil!", "korean": "호랑이도 제 말하면 온다더니!" },
  { "id": 180, "english": "Heads or tails!", "korean": "(동전) 앞면일까 뒷면일까!" },
  { "id": 181, "english": "Just kicking in.", "korean": "그냥 쉬고 있어." },
  { "id": 182, "english": "Put yourself out there.", "korean": "당당하게 나서봐. 자신있게 시도해봐." },
  { "id": 183, "english": "You're all I've got.", "korean": "나한텐 너뿐이야." },
  { "id": 184, "english": "That figures.", "korean": "어쩐지. 그럴 줄 알았어." },
  { "id": 185, "english": "What's there to think about?", "korean": "생각할 게 뭐있어? 뭘 고민해?" },
  { "id": 186, "english": "It's out of my hands.", "korean": "나도 어쩔 수 없어." },
  { "id": 187, "english": "Old habits die hard.", "korean": "세 살 버릇 여든까지 간다." },
  { "id": 188, "english": "Is that what I think it is?", "korean": "이게 내가 생각하는 게 맞아?" },
  { "id": 189, "english": "Just go with it.", "korean": "그냥 참고 지내." },
  { "id": 190, "english": "In one ear and out the other.", "korean": "한 귀로 듣고 한 귀로 흘려버려." },
  { "id": 191, "english": "Like father, like son.", "korean": "그 아버지에 그 아들이야." },
  { "id": 192, "english": "We're not done here.", "korean": "아직 얘기 안 끝났어." },
  { "id": 193, "english": "Here goes nothing.", "korean": "밑져야 본전이지." },
  { "id": 194, "english": "It's now or never.", "korean": "지금이 절호의 기회야." },
  { "id": 195, "english": "I didn't sleep a wink.", "korean": "한숨도 못 잤어." },
  { "id": 196, "english": "I'm just having a bad day.", "korean": "오늘 일진이 좀 안 좋아." },
  { "id": 197, "english": "You can't be serious.", "korean": "설마 진심은 아니겠지?" },
  { "id": 198, "english": "Get the picture?", "korean": "이해가 돼?" },
  { "id": 199, "english": "You look awful.", "korean": "꼴이 말이 아니네." },
  { "id": 200, "english": "Don't fall for it.", "korean": "속지 마!" }
]